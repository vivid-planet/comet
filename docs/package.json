{
    "name": "comet-docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "npm run generate-story-code-files && docusaurus build",
        "clear": "docusaurus clear",
        "deploy": "docusaurus deploy",
        "docusaurus": "docusaurus",
        "lint": "run-p lint:prettier lint:eslint lint:spelling",
        "lint:eslint": "eslint --max-warnings 0 src/ package.json",
        "lint:prettier": "npx prettier --check './**/*.{js,json,md,yml,yaml}'",
        "lint:spelling": "$npm_execpath cspell docs/**",
        "generate-story-code-files": "ts-node src/utils/generateStoryCodeFiles.ts",
        "watch-story-files": "chokidar -s \"../storybook/src/component-docs/**/*.stories.tsx\" -c \"npm run generate-story-code-files\"",
        "serve": "docusaurus serve",
        "start": "npm run generate-story-code-files && run-p watch-story-files \"docusaurus start --port 3300\"",
        "swizzle": "docusaurus swizzle",
        "write-heading-ids": "docusaurus write-heading-ids",
        "write-translations": "docusaurus write-translations"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@algolia/client-search": "^4.18.0",
        "@apollo/client": "^3.7.0",
        "@comet/admin": "workspace:*",
        "@comet/admin-color-picker": "workspace:*",
        "@comet/admin-date-time": "workspace:*",
        "@comet/admin-icons": "workspace:*",
        "@comet/admin-react-select": "workspace:*",
        "@comet/admin-rte": "workspace:*",
        "@comet/admin-theme": "workspace:*",
        "@docusaurus/core": "2.4.1",
        "@docusaurus/preset-classic": "2.4.1",
        "@docusaurus/theme-common": "2.4.1",
        "@docusaurus/theme-live-codeblock": "2.4.1",
        "@docusaurus/theme-mermaid": "2.4.1",
        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "@fontsource/roboto": "^4.2.3",
        "@mdx-js/react": "^1.6.22",
        "@mui/material": "^5.0.0",
        "@mui/system": "^5.0.0",
        "@mui/x-data-grid": "^5.17.6",
        "change-case": "^5.4.4",
        "draft-js": "^0.11.7",
        "final-form": "^4.20.9",
        "graphql": "^15.0.0",
        "history": "^4.10.1",
        "prism-react-renderer": "^1.3.5",
        "raw-loader": "^4.0.2",
        "react": "^17.0.2",
        "react-dnd": "^16.0.0",
        "react-dnd-html5-backend": "^16.0.0",
        "react-dom": "^17.0.2",
        "react-final-form": "^6.5.9",
        "react-intl": "^6.0.0",
        "react-live": "^2.4.1",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "react-select": "^3.2.0",
        "search-insights": "^2.7.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.8",
        "@comet/eslint-config": "workspace:*",
        "@docusaurus/module-type-aliases": "2.4.1",
        "@docusaurus/types": "2.4.1",
        "@tsconfig/docusaurus": "^1.0.5",
        "@types/react": "^17.0.48",
        "@types/react-dom": "^17.0.17",
        "chokidar-cli": "^2.0.0",
        "cspell": "^8.15.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "sucrase": "^3.32.0",
        "ts-morph": "^16.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.7.4",
        "webpack": "^5.88.1"
    }
}
