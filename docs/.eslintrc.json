{
    "extends": "@comet/eslint-config/react",
    "ignorePatterns": [".docusaurus", "build"],
    "rules": {
        "@calm/react-intl/missing-formatted-message": "off",
        "react/react-in-jsx-scope": "off"
    },
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    }
}
