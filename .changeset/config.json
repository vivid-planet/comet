{
    "$schema": "https://unpkg.com/@changesets/config@2.3.0/schema.json",
    "changelog": "@changesets/cli/changelog",
    "commit": false,
    "fixed": [["@comet/*"]],
    "linked": [],
    "access": "restricted",
    "baseBranch": "main",
    "updateInternalDependencies": "patch",
    "ignore": [
        "comet-storybook",
        "comet-demo-api",
        "comet-demo-admin",
        "comet-demo-admin-server",
        "comet-demo-site",
        "comet-demo-site-pages",
        "comet-docs"
    ],
    "snapshot": {
        "useCalculatedVersion": true
    }
}
