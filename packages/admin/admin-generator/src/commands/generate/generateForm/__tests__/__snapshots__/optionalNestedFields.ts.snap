    import isEqual from "lodash.isequal";

    

    

    type FormValues = Omit<GQLProductFormFragment, "address"> & { address: Omit<GQLProductFormFragment["address"], "zip"> & { zip: string;  }addressEnabled: boolean;  };

    interface FormProps {
            onCreate?: (id: string) => void;
id?: string;
