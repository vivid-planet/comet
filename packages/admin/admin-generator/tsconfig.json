{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["ES2023"],
        "module": "preserve",
        "moduleResolution": "bundler",
        "noEmit": true,
        "rootDir": "src",
        "strict": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": false,
        "target": "esnext"
    },
    "exclude": ["node_modules"],
    "extends": "../../../tsconfig.core.json",
    "include": ["./src"]
}
