{
    "name": "@comet/admin-icons",
    "version": "8.0.0-beta.1",
    "repository": {
        "directory": "packages/admin/admin-icons",
        "type": "git",
        "url": "https://github.com/vivid-planet/comet"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "run-s clean generate-icons && run-p build:babel build:types && eslint --fix src/generated/",
        "build:babel": "npx babel ./src -x \".ts,.tsx\" -d lib",
        "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly",
        "clean": "rimraf lib",
        "generate-icons": "ts-node generate-icons.ts",
        "lint": "run-p lint:prettier lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/ **/*.json --no-warn-ignored",
        "lint:prettier": "npx prettier --check './**/*.{js,json,md,yml,yaml}'",
        "lint:tsc": "tsc --noEmit",
        "start": "$npm_execpath run generate-icons && run-p start:babel start:types",
        "start:babel": "npx babel ./src -x \".ts,.tsx\" -d lib -w",
        "start:types": "tsc --project ./tsconfig.json --emitDeclarationOnly --watch --preserveWatchOutput"
    },
    "dependencies": {
        "use-constant": "^1.1.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.10",
        "@comet/admin-babel-preset": "workspace:*",
        "@comet/eslint-config": "workspace:*",
        "@mui/material": "^6.4.7",
        "@types/cli-progress": "^3.11.6",
        "@types/node": "^22.13.10",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "change-case": "^4.1.2",
        "cli-progress": "^3.12.0",
        "eslint": "^9.22.0",
        "fast-xml-parser": "^4.5.3",
        "npm-run-all2": "^5.0.2",
        "prettier": "^3.5.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    },
    "peerDependencies": {
        "@mui/material": "^6.0.0",
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
