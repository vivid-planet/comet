{
    "name": "@comet/admin-icons",
    "version": "8.7.1",
    "description": "A collection of icons for Comet Admin",
    "repository": {
        "directory": "packages/admin/admin-icons",
        "type": "git",
        "url": "https://github.com/vivid-planet/comet"
    },
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm run generate-icons && pnpm exec tsdown",
        "generate-icons": "ts-node generate-icons.ts",
        "lint": "run-p lint:prettier lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/ **/*.json --no-warn-ignored",
        "lint:prettier": "pnpm exec prettier --check './**/*.{js,json,md,yml,yaml}'",
        "lint:tsc": "tsc --noEmit",
        "start": "pnpm run generate-icons && pnpm exec tsdown --watch"
    },
    "dependencies": {
        "use-constant": "^1.1.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@comet/eslint-config": "workspace:*",
        "@mui/material": "^7.2.0",
        "@tsconfig/strictest": "^2.0.8",
        "@types/cli-progress": "^3.11.6",
        "@types/node": "^24.9.1",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "change-case": "^4.1.2",
        "cli-progress": "^3.12.0",
        "eslint": "^9.30.1",
        "fast-xml-parser": "^5.2.5",
        "npm-run-all2": "^8.0.0",
        "prettier": "^3.6.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "tsdown": "^0.16.7",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "@mui/material": "^7.0.0",
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
