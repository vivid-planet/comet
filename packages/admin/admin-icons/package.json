{
    "name": "@comet/admin-icons",
    "version": "2.0.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/vivid-planet/comet-admin",
        "directory": "packages/admin/admin-icons"
    },
    "scripts": {
        "clean": "rimraf lib && rimraf src/generated",
        "generate-icons": "ts-node generate-icons.ts",
        "build": "run-s clean generate-icons && run-p build:babel build:types",
        "build:babel": "npx babel ./src -x \".ts,.tsx\" -d lib",
        "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly",
        "start": "run-s clean generate-icons && run-p start:babel start:types",
        "start:babel": "npx babel ./src -x \".ts,.tsx\" -d lib -w",
        "start:types": "tsc --project ./tsconfig.json --emitDeclarationOnly --watch --preserveWatchOutput",
        "lint": "run-p lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/",
        "lint:tsc": "tsc --noEmit"
    },
    "peerDependencies": {
        "@mui/material": "^5.0.0",
        "react": "^17.0",
        "react-dom": "^17.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@comet/admin-babel-preset": "^2.0.0",
        "@comet/eslint-config": "^2.0.0",
        "@types/cli-progress": "^3.8.0",
        "cli-progress": "^3.8.2",
        "eslint": "^8.0.0",
        "fast-xml-parser": "^3.18.0",
        "pascal-case": "^3.1.2",
        "prettier": "^2.0.0",
        "rimraf": "^3.0.2",
        "ts-node": "^8.8.1",
        "typescript": "^4.0.0",
        "yarn-run-all": "^3.0.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
