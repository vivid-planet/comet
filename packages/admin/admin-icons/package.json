{
    "name": "@comet/admin-icons",
    "version": "2.0.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/vivid-planet/comet-admin",
        "directory": "packages/admin/admin-icons"
    },
    "scripts": {
        "clean": "rimraf lib && rimraf src/generated",
        "generate-icons": "ts-node generate-icons.ts",
        "build": "yarn clean && yarn generate-icons && tsc",
        "start": "yarn clean && yarn generate-icons && tsc --project ./tsconfig.json --watch --preserveWatchOutput",
        "lint": "run-p lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/",
        "lint:tsc": "tsc"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.0.0",
        "react": "^16.8.0 || ^17.0",
        "react-dom": "^16.8.0 || ^17.0"
    },
    "devDependencies": {
        "@comet/eslint-config": "^2.0.0",
        "@types/cli-progress": "^3.8.0",
        "cli-progress": "^3.8.2",
        "eslint": "^7.0.0",
        "fast-xml-parser": "^3.18.0",
        "pascal-case": "^3.1.2",
        "prettier": "^2.0.0",
        "rimraf": "^3.0.2",
        "ts-node": "^8.8.1",
        "typescript": "^4.0.0",
        "yarn-run-all": "^3.0.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
