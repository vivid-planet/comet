{
    "name": "@comet/admin-icons",
    "version": "8.12.0",
    "description": "A collection of icons for Comet Admin",
    "repository": {
        "directory": "packages/admin/admin-icons",
        "type": "git",
        "url": "https://github.com/vivid-planet/comet"
    },
    "type": "commonjs",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "run-s clean generate-icons && run-p build:babel build:types && eslint --fix src/generated/",
        "build:babel": "pnpm exec babel ./src -x \".ts,.tsx\" -d lib",
        "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly",
        "clean": "rimraf lib",
        "generate-icons": "ts-node generate-icons.ts",
        "lint": "run-p lint:prettier lint:eslint lint:tsc",
        "lint:ci": "pnpm run lint",
        "lint:eslint": "eslint --max-warnings 0 src/ **/*.json --no-warn-ignored",
        "lint:prettier": "pnpm exec prettier --check '*.{ts,js,json,md,yml,yaml}'",
        "lint:tsc": "tsc --noEmit",
        "start": "pnpm run generate-icons && run-p start:babel start:types",
        "start:babel": "pnpm exec babel ./src -x \".ts,.tsx\" -d lib -w",
        "start:types": "tsc --project ./tsconfig.json --emitDeclarationOnly --watch --preserveWatchOutput"
    },
    "dependencies": {
        "use-constant": "^2.0.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.6",
        "@babel/core": "^7.28.6",
        "@comet/admin-babel-preset": "workspace:*",
        "@comet/eslint-config": "workspace:*",
        "@mui/material": "^7.3.7",
        "@types/cli-progress": "^3.11.6",
        "@types/node": "^24.9.1",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "change-case": "^4.1.2",
        "cli-progress": "^3.12.0",
        "eslint": "^9.30.1",
        "fast-xml-parser": "^5.2.5",
        "npm-run-all2": "^8.0.4",
        "prettier": "^3.6.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^6.1.2",
        "ts-node": "^10.9.2",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "@mui/material": "^7.0.0",
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
