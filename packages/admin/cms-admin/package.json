        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "@mui/material": "^7.0.0",
        "@mui/x-data-grid": "^7.0.0",
        "final-form": "^4.0.0",
        "graphql": "^15.0.0 || ^16.0.0",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "react-dnd": "^16.0.0",
        "react-dnd-html5-backend": "^16.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
