        "jest-junit": "^16.0.0",
        "npm-run-all2": "^8.0.0",
        "prettier": "^3.6.2",
        "react": "^18.3.1",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.3.1",
        "react-final-form": "^6.5.9",
