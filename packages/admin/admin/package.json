{
    "name": "@comet/admin",
    "version": "8.14.0",
    "description": "Comet Admin package",
    "repository": {
        "directory": "packages/admin/admin",
        "type": "git",
        "url": "https://github.com/vivid-planet/comet"
    },
    "license": "BSD-2-Clause",
    "type": "commonjs",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
