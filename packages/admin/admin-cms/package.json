{
    "name": "@comet/admin-cms",
    "version": "2.0.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/*",
        "access-token-service-worker.ejs"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vivid-planet/comet-admin",
        "directory": "packages/admin/admin-cms"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "scripts": {
        "build": "rimraf lib && run-p generate-graphql-types generate-block-types && run-p build:babel build:types",
        "build:babel": "npx babel ./src -x \".ts,.tsx\" -d lib",
        "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly",
        "start": "rimraf lib && run-p generate-graphql-types generate-block-types && run-p start:babel start:types",
        "start:babel": "npx babel ./src -x \".ts,.tsx\" -d lib -w",
        "start:types": "tsc --project ./tsconfig.json --emitDeclarationOnly --watch --preserveWatchOutput",
        "lint": "run-p generate-graphql-types generate-block-types && run-p lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/",
        "lint:tsc": "tsc",
        "generate-graphql-types": "graphql-codegen",
        "generate-graphql-types:watch": "yarn generate-graphql-types --watch",
        "generate-block-types": "ts-node generate-block-types.ts",
        "generate-block-types:watch": "chokidar -s \"**/block-meta.json\" -c \"yarn generate-block-types\"",
        "test": "jest --verbose=true",
        "test:watch": "jest --watch"
    },
