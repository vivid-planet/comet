{
    "name": "comet-demo-admin",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "admin-generator": "rimraf --glob 'src/**/generated' && comet-admin-generator generate && $npm_execpath gql:types",
        "build": "run-s intl:compile && run-p gql:types generate-block-types && NODE_ENV=production vite build",
        "generate-block-types": "comet generate-block-types --inputs",
        "generate-block-types:watch": "chokidar -s \"block-meta.json\" -c \"npm run generate-block-types\"",
        "gql:types": "graphql-codegen",
        "gql:watch": "graphql-codegen --watch",
        "intl:compile": "run-p intl:compile:comet intl:compile:comet-demo",
        "intl:compile:comet": "formatjs compile-folder --format simple --ast lang/comet-lang lang-compiled/comet-lang",
        "intl:compile:comet-demo": "formatjs compile-folder --format simple --ast lang/comet-demo-lang/admin lang-compiled/comet-demo-lang-admin",
        "intl:extract": "formatjs extract \"src/**/*.{ts,tsx}\" --ignore ./**.d.ts --out-file lang-extracted/en.json --format simple",
        "lint": "run-s intl:compile && run-p gql:types generate-block-types && run-p lint:prettier lint:eslint lint:tsc && $npm_execpath lint:generated-files-not-modified",
        "lint:eslint": "eslint --max-warnings 0 src/ **/*.json --no-warn-ignored",
        "lint:generated-files-not-modified": "$npm_execpath admin-generator && git diff --exit-code HEAD -- src/**/generated",
        "lint:prettier": "npx prettier --check './**/*.{js,json,md,yml,yaml}'",
        "lint:tsc": "tsc --project .",
        "preview": "npm run build && vite preview",
        "serve": "node server",
        "start": "run-s intl:compile && run-p gql:types generate-block-types && dotenv -e .env.secrets -e .env.local -e .env -e .env.site-configs -- vite"
    },
    "dependencies": {
        "@apollo/client": "^3.13.4",
        "@comet/admin": "workspace:*",
        "@comet/admin-date-time": "workspace:*",
        "@comet/admin-icons": "workspace:*",
        "@comet/cms-admin": "workspace:*",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@fontsource-variable/roboto-flex": "^5.2.5",
        "@mui/material": "^6.4.7",
        "@mui/system": "^6.4.7",
        "@mui/x-data-grid": "^7.27.3",
        "@mui/x-data-grid-pro": "^7.27.3",
        "change-case": "^5.2.0",
        "dnd-core": "^16.0.1",
        "final-form": "^4.20.10",
        "graphql": "^15.10.1",
        "graphql-tag": "^2.12.6",
        "lodash.isequal": "^4.5.0",
        "rdndmb-html5-to-touch": "^8.1.2",
        "react": "^18.3.1",
        "react-app-polyfill": "^1.0.6",
        "react-dnd": "^16.0.1",
        "react-dnd-multi-backend": "^8.1.2",
        "react-dom": "^18.3.1",
        "react-final-form": "^6.5.9",
        "react-intl": "^7.1.6",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4",
        "use-debounce": "^10.0.4",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@comet/admin-generator": "workspace:*",
        "@comet/cli": "workspace:*",
        "@comet/eslint-config": "workspace:*",
        "@formatjs/cli": "^6.6.1",
        "@graphql-codegen/add": "^5.0.3",
        "@graphql-codegen/cli": "^5.0.5",
        "@graphql-codegen/fragment-matcher": "^5.1.0",
        "@graphql-codegen/introspection": "^4.0.3",
        "@graphql-codegen/named-operations-object": "^3.1.0",
        "@graphql-codegen/near-operation-file-preset": "^3.0.0",
        "@graphql-codegen/typescript": "^4.1.5",
        "@graphql-codegen/typescript-operations": "^4.5.1",
        "@parcel/watcher": "^2.5.1",
        "@swc/plugin-emotion": "^8.7.2",
        "@types/lodash.isequal": "^4.5.8",
        "@types/node": "^22.13.10",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react-swc": "^3.7.2",
        "chokidar-cli": "^3.0.0",
        "cosmiconfig-toml-loader": "^1.0.0",
        "dotenv-cli": "^7.4.4",
        "eslint": "^9.22.0",
        "npm-run-all2": "^5.0.2",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "typescript": "^5.7.3",
        "vite": "^5.4.14",
        "vite-plugin-html": "^3.2.2"
    }
}
